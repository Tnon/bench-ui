{
  "name": "bench-ui",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build:package": "vite build && vite build --config packages/vite.package.ts",
    "prepare": "husky install && yarn huskyInit",
    "huskyInit": "npx husky add .husky/pre-commit \"npm run lint:commit\" && yarn husky add .husky/commit-msg 'npx --no-install commitlint --edit \"$1\"'",
    "lint:eslint": "eslint --cache --max-warnings 0 src/**/*.{vue,ts,tsx} --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{ts,json,tsx,css,less,scss,vue,html}\"",
    "lint:commit": "lint-staged"
  },
  "dependencies": {
    "vue": "^3.2.25",
    "vue-router": "^4.0.13",
    "vite-plugin-md": "^0.11.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "@vue/compiler-sfc": "^3.2.20",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vuedx/typecheck": "^0.6.3",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.2.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "gh-pages": "^3.1.0",
    "husky": "^7.0.4",
    "less": "^4.1.1",
    "less-loader": "^8.1.1",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "sass": "^1.32.12",
    "sass-loader": "^11.0.1",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.29.8"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,less,styl,html}": [
      "prettier --write"
    ]
  }
}
